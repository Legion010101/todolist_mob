{
  "name": "TodoList",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "lint:write": "yarn prettier --write src/**/*.{ts,tsx} && yarn eslint src/ --fix --ext .ts,.tsx",
    "lint:check": "yarn prettier --check src/**/*.{ts,tsx} && yarn eslint src/ --ext .ts,.tsx --color",
    "prepare": "husky install && chmod +x .husky/pre-commit"
  },
  "lint-staged": {
    "src/**/*.(js|jsx|ts|tsx)": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ],
    "src/**/*.(css|scss|md)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.9",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "@reduxjs/toolkit": "^1.9.7",
    "nanoid": "^5.0.1",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-rename": "^3.2.13",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-splash-screen": "^3.3.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "lint-staged": "^15.0.2",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
